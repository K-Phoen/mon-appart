<!doctype html>
<html class="no-js" lang="fr">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{% block title %}Tu veux un appart ?{% endblock %}</title>

    {% block stylesheets %}
      {% stylesheets
        '@AppBundle/Resources/public/css/vendor/foundation.css'
        '@AppBundle/Resources/public/css/style.css'
      %}
      <link rel="stylesheet" href="{{ asset_url }}" />
      {% endstylesheets %}
      <link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    {% endblock %}

    {% block javascripts %}
      {% javascripts
        '@AppBundle/Resources/public/js/vendor/jquery.js'
        '@AppBundle/Resources/public/js/vendor/foundation.min.js'
      %}
      <script src="{{ asset_url }}"></script>
      {% endjavascripts %}
    {% endblock %}
</head>
<body>
<div class="row">
    <div class="large-12 columns">
        <h1>Tu veux un appart ?</h1>
    </div>
</div>

<div class="row">
    <div class="large-12 columns">
        {% block body %}{% endblock %}
    </div>
</div>

<script src="{{ asset('bundles/fosjsrouting/js/router.js') }}"></script>
<script src="{{ path('fos_js_routing_js', {"callback": "fos.Router.setData"}) }}"></script>
{% block footer_javascripts %}
  {% javascripts
    '@AppBundle/Resources/public/js/app.js'
  %}
  <script src="{{ asset_url }}"></script>
  {% endjavascripts %}
{% endblock %}
</body>
</html>
